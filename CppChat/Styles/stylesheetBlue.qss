  color: #f9faf0;
  font-size: 15px;
}

#acceptBtn:hover, #cancleBtn:hover {
  text-align: center;
  border-radius: 5px;
  background-color: #48b2e5;
  color: #cefdec;
  font-size: 15px;
}

#acceptBtn:pressed, #cancleBtn:pressed {
  text-align: center;
  border-radius: 5px;
  background-color: #034ec4;
  color: #cefdec;
  font-size: 15px;
}

